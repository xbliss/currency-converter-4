{
  "name": "currency-converter-chrome-extension",
  "version": "0.3.0",
  "description": "Currency converter extension for chrome",
  "author": "Zach Orlovsky <sadorlovsky@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/sadorlovsky/currency-converter-chrome-extension.git"
  },
  "scripts": {
    "lint": "standard --verbose | snazzy",
    "start": "webpack --config webpack.dev.config --watch",
    "build:dev": "webpack --config webpack.dev.config",
    "build:prod": "NODE_ENV=production webpack --config webpack.prod.config -p",
    "pack": "bestzip package.zip app/*",
    "precommit": "npm run lint",
    "prepush": "npm run lint"
  },
  "standard": {
    "parser": "babel-eslint",
    "globals": [
      "chrome"
    ]
  },
  "dependencies": {
    "classnames": "^2.2.3",
    "isomorphic-fetch": "^2.2.1",
    "lodash": "^4.7.0",
    "lscache": "^1.0.5",
    "react": "^0.14.8",
    "react-chrome-redux": "0.0.7",
    "react-css-modules": "^3.7.6",
    "react-dom": "^0.14.8",
    "react-icons": "^1.0.3",
    "react-spinkit": "^1.1.4",
    "redux-thunk": "^2.0.1"
  },
  "devDependencies": {
    "babel-core": "^6.7.4",
    "babel-eslint": "^6.0.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-react-jsx": "^6.7.4",
    "babel-plugin-transform-runtime": "^6.6.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "bestzip": "^1.1.3",
    "css-loader": "^0.23.1",
    "husky": "^0.11.4",
    "postcss": "^5.0.19",
    "postcss-cssnext": "^2.5.1",
    "postcss-loader": "^0.8.2",
    "snazzy": "^3.0.0",
    "style-loader": "^0.13.1",
    "sugarss": "^0.1.2"
  }
}
