{
  "name": "@sadorlovsky/currency-converter",
  "version": "0.6.0",
  "description": "Currency converter extension for chrome",
  "author": "Zach Orlovsky <sadorlovsky@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/sadorlovsky/currency-converter.git"
  },
  "scripts": {
    "lint": "eslint src test",
    "test": "CONFIG=$(pwd)/webpack.test.config.js BABEL_DISABLE_CACHE=1 NODE_ENV=TEST nyc ava",
    "coveralls": "nyc report --reporter=text-lcov | coveralls",
    "start": "webpack --config webpack.dev.config --watch",
    "build:dev": "webpack --config webpack.dev.config",
    "build:prod": "NODE_ENV=production webpack --config webpack.prod.config -p",
    "pack": "bestzip package.zip app/*",
    "precommit": "npm run lint && npm run test",
    "prepush": "npm run lint && npm run test"
  },
  "ava": {
    "files": [
      "test",
      "src/popup/**/*.test.js"
    ],
    "babel": "inherit",
    "require": [
      "babel-register"
    ]
  },
  "nyc": {
    "include": [
      "src/**/*.js"
    ]
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "isomorphic-fetch": "^2.2.1",
    "lodash": "^4.12.0",
    "lscache": "^1.0.5",
    "react": "^15.0.2",
    "react-css-modules": "^3.7.6",
    "react-dom": "^15.0.2",
    "react-icons": "^2.0.1",
    "react-redux": "^4.4.5",
    "react-spinkit": "^1.1.7",
    "redux": "^3.5.2",
    "redux-actions": "^0.9.1",
    "redux-thunk": "^2.1.0"
  },
  "devDependencies": {
    "@sadorlovsky/eslint-config": "^0.7.0",
    "ava": "^0.14.0",
    "babel-core": "^6.8.0",
    "babel-eslint": "^6.0.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-react-jsx": "^6.8.0",
    "babel-plugin-transform-runtime": "^6.8.0",
    "babel-plugin-webpack-loaders": "^0.4.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.8.0",
    "babel-runtime": "^6.6.1",
    "bestzip": "^1.1.3",
    "coveralls": "^2.11.9",
    "css-loader": "^0.23.1",
    "enzyme": "^2.3.0",
    "eslint": "^2.9.0",
    "eslint-plugin-ava": "^2.3.1",
    "eslint-plugin-import": "^1.8.0",
    "eslint-plugin-react": "^5.1.1",
    "husky": "^0.11.4",
    "nock": "^8.0.0",
    "nyc": "^6.4.4",
    "postcss": "^5.0.21",
    "postcss-cssnext": "^2.5.2",
    "postcss-loader": "^0.9.1",
    "react-addons-test-utils": "^15.0.2",
    "react-dom": "^15.0.2",
    "redux-mock-store": "^1.0.2",
    "style-loader": "^0.13.1",
    "sugarss": "^0.1.3",
    "webpack": "^1.13.0"
  }
}
